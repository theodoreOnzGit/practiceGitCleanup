FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];


internalField   uniform 3.75e-05;

boundaryField
{
    topPatch
    {
        type            zeroGradient;
    }
    wall
    {
        type            kqRWallFunction;
        value           uniform 3.75e-05;
    }
    bottomPatch
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           nonuniform 0();
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 3.75e-05;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 3.75e-05;
    }
}


